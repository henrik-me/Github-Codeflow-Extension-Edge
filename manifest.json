{
      "manifest_version": 2,
      "name": "Codeflow",
      "version": "1.0",
      "author" : "henrikm",
    
      "description": "Enables CodeFlow Integration for Github PR's.",
    
      "icons": {
        "25": "icons/icon.png"
      },
    
      "content_scripts": [
        {
          "matches": ["https://github.com/*", "https://*.github.com/*"],
          "js": ["codeflow.js"],
          "run_at": "document_idle"
        }
      ]
    }